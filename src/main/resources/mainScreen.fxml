<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TabPane prefHeight="120.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Untitled Tab 1">
               <content>
                  <VBox>
                     <children>
                        <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0">
                           <children>
                              <CheckBox fx:id="play" mnemonicParsing="false" text="Play" />
                              <CheckBox fx:id="comic" layoutX="20.0" layoutY="25.0" mnemonicParsing="false" text="Comic">
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                              </CheckBox>
                           </children>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <padding>
                              <Insets left="10.0" />
                           </padding>
                        </HBox>
                        <HBox layoutX="10.0" layoutY="10.0" prefHeight="100.0" prefWidth="200.0" />
                     </children>
                  </VBox>
               </content>
          </Tab>
        </tabs>
      </TabPane>
      <Canvas fx:id="viewport" height="720.0" width="1280.0" />
   </children>
</VBox>
